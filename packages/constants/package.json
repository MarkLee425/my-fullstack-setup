{
	"name": "@my-fullstack-setup/constants",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "tsc --watch",
		"build": "tsdown",
		"clean": "rimraf dist .turbo node_modules"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./password": {
			"types": "./src/password.ts",
			"default": "./dist/password.js"
		}
	},
	"turbo": {
		"outputs": [
			"dist/**"
		]
	},
	"devDependencies": {
		"@types/node": "^22.14.0",
		"@my-fullstack-setup/ts-config": "workspace:*",
		"tsdown": "^0.14.1",
		"typescript": "latest"
	}
}
