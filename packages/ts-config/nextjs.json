{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"declaration": false,
		"declarationMap": false,
		"lib": ["dom", "dom.iterable", "esnext"],
		"plugins": [{ "name": "next" }],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"jsx": "preserve",
		"noEmit": true,
		"skipLibCheck": true,
		"incremental": true
	}
}
