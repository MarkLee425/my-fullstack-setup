{
	"name": "my-fullstack-setup",
	"private": true,
	"packageManager": "pnpm@10.14.0",
	"scripts": {
		"check": "biome check --write .",
		"prepare": "husky",
		"dev": "turbo dev",
		"build": "turbo build",
		"clean": "turbo clean",
		"format": "biome format . --write",
		"check-types": "turbo check-types",
		"dev:native": "turbo -F native dev",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F server dev",
		"db:push": "turbo -F server db:push",
		"db:migrate:": "turbo -F server db:migrate:",
		"db:migrate:up": "turbo -F server db:migrate:up",
		"db:migrate:down": "turbo -F server db:migrate:down",
		"db:migrate:down:all": "turbo -F server db:migrate:down:all",
		"db:migrate:full": "turbo -F server db:migrate:full",
		"db:up": "turbo -F server db:up",
		"db:down": "turbo -F server db:down",
		"redis:up": "turbo -F server redis:up",
		"redis:down": "turbo -F server redis:down",
		"setup": "turbo -F server setup"
	},
	"dependencies": {},
	"devDependencies": {
		"turbo": "^2.5.4",
		"@biomejs/biome": "^2.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --write ."
		]
	}
}
